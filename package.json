  "scripts": {
    "babel": "npx babel --config-file ./esbuild.babelrc build/esbuild/tasks/webcrawl/webcrawl.js --out-file build/resources/main/tasks/webcrawl/webcrawl.js",
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:static": "npx tsup -d build/resources/main/static",
    "build:tsup": "npx tsup -d build/resources/main",
    "clean": "del-cli build coverage",
    "concat:graphql": "n-concat src/main/resources/lib/esbuild/getOwnPropertyDescriptors.js build/esbuild/services/graphQL/graphQL.js | n-pipe build/concat/services/graphQL/graphQL.js",
    "concat:init": "n-concat src/main/resources/lib/esbuild/getOwnPropertyDescriptors.js build/esbuild/tasks/init/init.js | n-pipe build/concat/tasks/init/init.js",
    "concat:migrate": "n-concat src/main/resources/lib/esbuild/getOwnPropertyDescriptors.js build/esbuild/tasks/migrate/migrate.js | n-pipe build/concat/tasks/migrate/migrate.js",
    "concat:webcrawl": "n-concat src/main/resources/lib/esbuild/getOwnPropertyDescriptors.js build/esbuild/tasks/webcrawl/webcrawl.js | n-pipe build/concat/tasks/webcrawl/webcrawl.js",
    "cov": "jest --no-cache --coverage",
    "esbuild:graphql": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --format=cjs --minify --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json src/main/resources/services/graphQL/graphQL.ts",
    "esbuild:init": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --external:/services/graphQL/graphQL --format=cjs --minify --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json src/main/resources/tasks/init/init.ts",
    "esbuild:migrate": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --external:/services/graphQL/graphQL --format=cjs --minify --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json src/main/resources/tasks/migrate/migrate.ts",
    "esbuild:webcrawl": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --format=cjs --minify --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json src/main/resources/tasks/webcrawl/webcrawl.ts",
    "esbuild": "npm-run-all --parallel esbuild:graphql esbuild:init esbuild:migrate esbuild:webcrawl",
    "esbuild:all": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --format=cjs --minify --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json $(find src/main/resources/ -name '*.ts' -a ! -name '*.d.ts')",
    "eslint": "npx eslint src/main/resources/**/*.ts src/main/resources/**/*.tsx",
    "doctoc": "npx doctoc --github README.md",
    "prebabel": "npm run esbuild:webcrawl",
    "preconcat:graphql": "npm run esbuild:graphql",
    "preconcat:init": "npm run esbuild:init",
    "preconcat:migrate": "npm run esbuild:migrate",
    "preconcat:webcrawl": "npm run esbuild:webcrawl",
    "preswc:graphql": "npm run concat:graphql",
    "preswc:init": "npm run concat:init",
    "preswc:migrate": "npm run concat:migrate",
    "preswc:webcrawl": "npm run concat:webcrawl",
    "pretestold": "npm run tsc",
    "pretsc": "npm run clean",
    "swc:graphql": "npx swc --config-file ./.swcrc.json build/concat/services/graphQL/graphQL.js -o build/resources/main/services/graphQL/graphQL.js",
    "swc:init": "npx swc --config-file ./.swcrc.json build/concat/tasks/init/init.js -o build/resources/main/tasks/init/init.js",
    "swc:migrate": "npx swc --config-file ./.swcrc.json build/concat/tasks/migrate/migrate.js -o build/resources/main/tasks/migrate/migrate.js",
    "swc:webcrawl": "npx swc --config-file ./.swcrc.json build/concat/tasks/webcrawl/webcrawl.js -o build/resources/main/tasks/webcrawl/webcrawl.js",
    "swc": "npm-run-all --parallel swc:graphql swc:init swc:migrate swc:webcrawl",
    "test": "jest --no-cache",
    "tsc": "npx tsc --pretty",
    "verify:types": "npm run verify:types:assets && npm run verify:types:server",
    "verify:types:assets": "npx tsc --noEmit -p src/main/resources/assets/tsconfig.json",
    "verify:types:server": "npx tsc --noEmit -p src/main/resources/tsconfig.json",
    "watch": "node_modules/webpack-cli/bin/cli.js -w --stdin",
    "webpack": "node_modules/webpack-cli/bin/cli.js"
  },
  "type": "commonjs"
}
