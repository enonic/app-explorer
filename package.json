  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:explorer": "npx tsup --config src/explorer/tsup.config.ts",
    "build:static": "npx tsup -d build/resources/main/static",
    "build:tsup": "npx tsup -d build/resources/main",
    "build:webcrawlerform": "vite build src/webcrawler/form",
    "build:webcrawlertask": "npx tsup --config src/webcrawler/task/tsup.config.ts",
    "clean": "del-cli build coverage",
    "compress:assets": "precompress build/resources/main/assets",
    "compress:explorerAssets": "precompress build/resources/main/services/explorerAssets/files",
    "cov": "jest --no-cache --coverage",
    "eslint": "npx eslint src/main/resources/**/*.ts src/main/resources/**/*.tsx",
    "doctoc": "npx doctoc --github README.md",
    "postbuild:explorer": "npm run compress:explorerAssets",
    "postbuild:static": "precompress  build/resources/main/static",
    "pretestold": "npm run tsc",
    "pretsc": "npm run clean",
    "test": "jest --no-cache",
    "tsc": "npx tsc --pretty",
    "verify:types": "concurrently -c auto -g --timings npm:verify:types:*",
    "verify:types:assets": "npx tsc --noEmit -p tsconfig.assets.json",
    "verify:types:server": "npx tsc --noEmit -p tsconfig.xp.nashorn.json"
  },
