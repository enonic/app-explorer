  "scripts": {
    "babel": "npx babel --config-file ./esbuild.babelrc build/esbuild/tasks/webcrawl/webcrawl.js --out-file build/resources/main/tasks/webcrawl/webcrawl.js",
    "clean": "rm -r build 2>/dev/null;true",
    "esbuild:webcrawl": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --format=cjs --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json src/main/resources/tasks/webcrawl/webcrawl.ts",
    "esbuild:all": "npx esbuild --bundle --external:/lib/cache --external:/lib/explorer* --external:/lib/galimatias --external:/lib/graphql* --external:/lib/guillotine* --external:/lib/http-client --external:/lib/license --external:/lib/router --external:/lib/util/* --external:/lib/xp/* --format=cjs --outbase=src/main/resources --outdir=build/esbuild --tsconfig=tsconfig.esbuild.json $(find src/main/resources/ -name '*.ts' -a ! -name '*.d.ts')",
    "eslint": "npx eslint src/main/resources/**/*.ts src/main/resources/**/*.tsx",
    "doctoc": "npx doctoc --github README.md",
    "mocha": "npx mocha",
    "prebabel": "yarn esbuild:webcrawl",
    "premocha": "yarn tsc",
    "preswc": "yarn esbuild:webcrawl",
    "pretest": "yarn tsc",
    "pretsc": "yarn clean",
    "swc": "npx swc --config-file ./.swcrc.json build/esbuild/tasks/webcrawl/webcrawl.js -o build/resources/main/tasks/webcrawl/webcrawl.js",
    "test": "npx mocha build/tsc/app-explorer/test/**/*.test.js",
    "tsc": "npx tsc --pretty",
    "watch": "node_modules/webpack-cli/bin/cli.js -w --stdin",
    "webpack": "node_modules/webpack-cli/bin/cli.js"
  }
